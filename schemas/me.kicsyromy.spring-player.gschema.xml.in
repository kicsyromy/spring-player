<?xml version="1.0" encoding="UTF-8"?>
<schemalist>

  <enum id="@APPLICATION_ID@.current-page">
    <value nick="albums_page" value="0"/>
    <value nick="artists_page" value="1"/>
    <value nick="genres_page" value="2"/>
    <value nick="songs_page" value="3"/>
